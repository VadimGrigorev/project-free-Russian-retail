      Сп2:=Сп.GetByName("Марки");
      For i:=1 To Сп2.Size() Do
        Тч.AddString(Сп2.GetName(i)+_TAB+Сп2[i]);
      EndDo;
      Тч.Show("Марки",,1);
      Exit;
    EndIf;
  EndIf;
  
  If Сп.Size()=1 Then
    OpenObject(Сп.Get(1),0,1);
  Else
    Об:=0;
    If Сп.Select(Об,0,"Выбор объекта")>0 Then
      OpenObject(Об,0,1);
    EndIf;
  EndIf;  
EndFunction

