    Form.ТСтроки.Value.Column("Номенклатура").isVisible:=1;
    Form.ТСтроки.Value.Column("оШтрихкод").isVisible:=1;
    Form.ТСтроки.Value.Column("Единицы").isVisible:=1;
    Form.ТСтроки.Value.Column("Партия").isVisible:=1;
